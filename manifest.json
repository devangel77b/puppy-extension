{
    "manifest_version": 2,
    "name": "Make America Puppies Again",
    "description": "Replaces images of Donald Trump with guide dog puppies.",
    "version": "0.0.0",
//   "update_url": "https://clients2.google.com/service/update2/crx",


    // for using jquery
    "background":{
	"scripts": ["jquery-3.1.1.min.js"]
    },
    "browser_action": {
	"default_icon": "up48.png",
	"default_popup": "ui.html"
    },
    "content_scripts": [ {
	"js": [ "jquery-3.1.1.min.js",
		"trumpuppies.js"],
	"matches": [ "http://*/*" ]
    } ],
    "icons": {
	"128": "up128.png",
	"48": "up48.png"
    },
    /*   "key": "", */


    
    "permissions": [
	//"activeTab",
	//"http://www.teaandkittens.co.uk/",
	//"https://ajax.googleapis.com/"
	//"http://www.jquery.com"
    ]
}
